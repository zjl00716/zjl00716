<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>第五周练习</title>
    <script>
        // 修正变量定义
        let a = 10;
        const title = "不不变的值";
        alert(title);
        let b = "10";
        console.log(a === b);
        console.log(a == b);
        console.log(a + b);
        console.log(a + Number(b));
        console.log(Number(a) + b);
        console.log(Number(a) + Number(b));
        let c = "10.00";
        console.log(a === c);
        console.log(a == c);
        console.log(a + b);
        console.log(a + Number(c));
        console.log(Number(a) + c);
        console.log(Number(a) + Number(c));
        // 修正变量定义
        let score = 100;
        if (score >= 90) {
            console.log("优秀");
        } else {
            console.log("良好或者及格以及不及格");
        }
        // 修正循环条件
        for (let i = 60; i > 10; i--) {
            console.log(i);
        }
        let score2 = 85;
        while (score2 > 85) {
            console.log("优秀");
            score2 = score2 - 10;
        }
        function sayHello() {
            console.log("这是见面打招呼的方式");
        }
        sayHello();
        // 修正参数名
        function sayHello2(hello) {
            console.log("这是见面打招呼的方式" + hello);
        }
        sayHello2("你好");
        // 修正参数名
        function sayHello3(hello, goodbye) {
            console.log("这是见面打招呼的方式" + hello + goodbye);
        }
        sayHello3("你好", "再见");
        // 修正参数名
        function sayHello4(hello1, goodbye, hello2) {
            console.log("这是见面打招呼的方式" + hello1 + goodbye + hello2);
        }
    </script>
